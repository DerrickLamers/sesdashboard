{
    "type": "project",
    "name": "nikeev/sesdashboard",
    "description": "SesDashboard is an Analytics UI for Amazon Simple Email Service.",
    "license": "MIT",
    "keywords": ["aws", "dashboard", "analytics", "amazon", "email", "transaction", "ses"],
    "homepage": "https://github.com/Nikeev/sesdashboard",
    "authors": [
        {
            "name": "Anton Nikeev",
            "email": "a@nikeev.com",
            "homepage": "https://nikeev.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Nikeev/sesdashboard/issues",
        "docs": "https://sesdashboard.readthedocs.io/"
    },
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-migrations-bundle": "2.*",
        "knplabs/knp-paginator-bundle": "^5.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/amazon-mailer": "5.3.*",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^v1.13.4",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/intl": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/monolog-bundle": "^v3.7.0",
        "symfony/notifier": "5.3.*",
        "symfony/orm-pack": "*",
        "symfony/process": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer-pack": "*",
        "symfony/string": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "5.3.*",
        "symfony/web-link": "5.3.*",
        "symfony/webpack-encore-bundle": "^v1.12.0",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "zenstruck/foundry": "^v1.12.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
